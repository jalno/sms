		"en_US": "langs/en_US.json"
	},
	"events": [
		{
			"name":"packages/userpanel/Events/UserTypePermissionsList",
			"listener": "packages/sms/Listeners/Settings/UserType@permissions_list"
		},
		{
			"name":"packages/userpanel/Events/Users/BeforeDelete",
			"listener": "packages/sms/Listeners/UserPanel/Users/BeforeDelete@check"
		},
		{
			"name":"packages/userpanel/Events/Search",
			"listener": "packages/sms/Listeners/Search@find"
		},
		{
			"name":"packages/notifications/Events/Channels",
			"listener": "packages/sms/Listeners/Notifications@channels"
		}
	]
